{
    "name": "@wayco/separate-html-webpack-plugin",
    "version": "1.0.0",
    "description": "webpack plugin, prevent some link or script inserted to index.html",
    "main": "src/SeparateHTMLWebpackPlugin.js",
    "scripts": {
        "test": "exit 1",
        "lint": "eslint --ext .js ./",
        "commit": "git-cz",
        "precommit": "lint-staged",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wkcole/SeparateHTMLWebpackPlugin.git"
    },
    "keywords": [
        "WebpackHTMLPlugin",
        "Webpack",
        "Plugin"
    ],
    "author": "wayco",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wkcole/SeparateHTMLWebpackPlugin/issues",
        "email": "wkcole@foxmail.com"
    },
    "homepage": "https://github.com/wkcole/SeparateHTMLWebpackPlugin#readme",
    "devDependencies": {
        "@commitlint/cli": "^6.1.3",
        "@commitlint/config-conventional": "^6.1.3",
        "commitizen": "^2.9.6",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "^4.18.2",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "husky": "^0.14.3",
        "lint-staged": "^7.0.0"
    },
    "dependencies": {
        "eslint": "^4.18.2",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "husky": "^0.14.3",
        "lint-staged": "^7.0.0"
    }
}
